{
  "name": "cachetools",
  "authors": [
    "Igor Khasilev"
  ],
  "dependencies": {
    "stdx-allocator": "~>2",
    "optional": "~>0"
  },
  "buildTypes": {
    "debug": {
      "buildOptions": [
        "debugMode",
        "debugInfo"
      ],
      "dflags-dmd": [
        "-g", "-gf", "-dip1000", "-dip1008"
      ],
      "dflags-ldc": [
        "-d-debug=cachetools"
      ]
    },
    "unittest": {
      "buildOptions": [
        "unittests", "debugMode", "debugInfo"
      ],
      "dflags-dmd": [
        "-g", "-gf", "-dip1000", "-dip1008"
      ],
      "dflags-ldc": [
        "-d-debug=cachetools"
      ],
      "debugVersions": [
        "cachetools"
      ]
    }
  },
  "sourcePaths": ["source"],
  "importPaths": ["source"],
  "description": "A minimal D application.",
  "license": "proprietary"
}
